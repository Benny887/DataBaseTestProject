/*create table customers(
             firstName varchar(15) not null check (firstName <> ''),
             lastName varchar(20) not null check (lastName <> '')  primary key);
			 
create table products(
			 prodName varchar(30) unique not null check(prodName <> '') primary key,
			 price int not null check (price >= 0));		
			 
create table purchases (
			 customer varchar(20) not null check(customer <> ''),
		 	 purchase varchar(30) not null check(purchase <> ''),
			 acquireDate date not null,
			 foreign key (purchase) references products(prodName),
			 foreign key (customer) references customers(lastName))*/		 
			 
